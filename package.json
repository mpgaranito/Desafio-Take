{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "cross-env CI=true yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "cross-env CI=true yarn test --findRelatedTests"
    ]
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "mocha tests/**/*.test.js --timeout 30000 --exit",
    "lint": "eslint --fix --ext .js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucioerlan/desafio-take.git"
  },
  "author": "Erlan Lucio",
  "license": "DesafioTake",
  "bugs": {
    "url": "https://github.com/lucioerlan/desafio-take/issues"
  },
  "homepage": "https://github.com/lucioerlan/desafio-take#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "express-redis-cache": "^1.1.3",
    "helmet": "^4.6.0",
    "ip": "^1.1.5",
    "moment-timezone": "^0.5.32",
    "ms": "^2.1.3",
    "node-emoji": "^1.10.0",
    "node-uuid": "^1.4.8",
    "swagger-ui-express": "^4.0.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.0.10",
    "lint-staged": "^10.0.0",
    "mocha": "^8.1.3",
    "nodemon": "^1.19.4"
  }
}
